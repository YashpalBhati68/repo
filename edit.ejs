<% layout("/layouts/boilerplate") %>
<body>
  <form method="POST" action="/listings/<%= listings._id%>?_method=PUT">
    <h3>Edit your listing</h3>
    <input name="listing[title]" value="<%=listings.title%>" type="text" />
    <br /><br />
    <textarea name="listing[description]" type="text">
<%=listings.description%></textarea
    >
    <br /><br />
    <input name="listing[image]" value="<%= listings.image.url%>" type="url" />
    <br /><br />
    <input name="listing[price]" value="<%= listings.price%>" type="text" />
    <br /><br />
    <input
      name="listing[location]"
      value="<%= listings.location%>"
      type="text"
    />
    <br /><br />
    <input name="listing[country]" value="<%=listings.country%>" type="text" />
    <br /><br />

    <button>Edit</button>
  </form>
</body>

